@startuml

class User {
	name : String
  getName(n : String)
  setName() : void
}

class Student {
	studentId : int
  class : String
	- getPeerEval() : int
  setPeerEval(n : int)
  getClass : String
}

class Teacher {
	teacherId : int
  getStudentEval() : int
	setStudentEval(n : int)
}

class Team {
	teamSize : int
	teamName : String
  getTeamName() : String
  setTeamName(n : String)
  getTeamSize() : int
}

class Question {
	questionID : int
  questionType: String
}

class Answer {
	answerID : int
  answerType : String
}

class Evaluation {
	evalTeamId : int
  evalID : int
}

User "1" <|-- "1" Teacher : is a
User "1" <|-- "1" Student : is a
Team "1" *-- "1..*" Student : makes a
Evaluation "1" <*-.- "1..*" Question : has
Question "1..*" <|-- "1..*" Answer : is
Student "1..*" <|-- "1..*" GradStudent : is

@enduml