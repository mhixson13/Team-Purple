@startuml

class User {
	name : String
  getName(n : String)
  setName() : void
}

class Student {
	studentId : int
  class : String
	getPeerEval() : int
  setPeerEval(n : int)
  getClass : String
}

class Teacher {
	teacherId : int
  getStudentEval() : int
	setStudentEval(n : int)
}

class Team {
	teamSize : int
	teamName : String
  getTeamName() : String
  setTeamName(n : String)
  getTeamSize() : int
}